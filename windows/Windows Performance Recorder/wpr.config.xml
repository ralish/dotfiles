<?xml version="1.0" encoding="utf-8"?>
<!--
  Windows Performance Recorder configuration
  https://learn.microsoft.com/en-au/windows-hardware/test/wpt/windows-performance-recorder

  Last reviewed release: Windows 11 23H2

  The XML schema is included in wpr.exe as an RCData resource.
-->
<WindowsPerformanceRecorder Version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="wpr.config.xsd">
  <Configuration>
    <MaxProfilesToStart Value="64" />
    <QueryRuntimePropertiesInterval Value="1000" />
    <SaveRecordingKeyboardShortcut WinKey="true" CtrlKey="true" ShiftKey="true" AltKey="false" CharKey="r" VirtualKeyCode="0" />
    <SaveWaypointsKeyboardShortcut WinKey="true" CtrlKey="true" ShiftKey="false" AltKey="true" CharKey="w" VirtualKeyCode="0" />
    <FireEventKeyboardShortcut WinKey="true" CtrlKey="true" ShiftKey="false" AltKey="true" CharKey="x" VirtualKeyCode="0" />
    <NGenPdbsCachePath Value="%ProgramData%\WindowsPerformanceRecorder\NGenPdbs_Cache" />
  </Configuration>
</WindowsPerformanceRecorder>