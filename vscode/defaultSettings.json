        "announcement": "auto"
    },

    // Plays a signal - sound (audio cue) and/or announcement (alert) - when a feature is cleared (for example, the terminal, Debug Console, or Output channel).
    "accessibility.signals.clear": {
        "sound": "auto",
        "announcement": "auto"
