{
    /*
     * Windows Terminal settings
     *
     * Last reviewed release: v1.23.12811.0
     * Default file path: %LOCALAPPDATA%\Packages\Microsoft.WindowsTerminal_8wekyb3d8bbwe\LocalState\settings.json
     *
     * Many Windows Terminal profiles are automatically generated and assigned
     * a unique GUID. The GUID may be taken directly from the underlying source
     * (e.g. WSL profiles) or be derived from selected inputs from the source
     * (e.g. Visual Studio profiles). In both cases, the GUID is generally not
     * portable between systems because Windows Terminal won't be able to map
     * the profile GUID in the settings file to its corresponding remaining
     * settings (either in a fragment file or internally generated) on which
     * the user profile settings will be overlaid to form the complete profile.
     *
     * Because of this we define this template settings file which should be
     * copied to `settings.json` and modified as needed for the given system.
     * Future changes to `template.json` will need to be manually merged in.
     */

    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",

    // Startup
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "language": null, // Default
    "defaultInputScope": "default",
    "firstWindowPreference": "defaultProfile",
    "windowingBehavior": "useNew",
    "initialCols": 120,
    "initialRows": 30,
    "launchMode": "maximized",
    "initialPosition": ",", // Let Windows decide
    "centerOnLaunch": true,

    // Interaction
    "copyOnSelect": true,
    "copyFormatting": "none",
    "trimBlockSelection": true,
    "trimPaste": true,
    "wordDelimiters": " /\\()\"'-.,:;<>~!@#$%^&*|+=[]{}~?\u2502",
    "snapToGridOnResize": true,
    "tabSwitcherMode": "inOrder",
    "focusFollowMouse": false,
    "experimental.detectURLs": true,
    "searchWebDefaultQueryUrl": "https://kagi.com/search?q=%22%s%22",
    "experimental.enableColorSelection": false,
    // Interaction: Warnings
    "warning.confirmCloseAllTabs": false,
    "warning.inputService": true,
    "warning.largePaste": true,
    "warning.multiLinePaste": true,

    // Appearance
    "theme": {
        "dark": "dark",
        "light": "light"
    },
    "newTabPosition": "afterLastTab",
    "showTabsInTitlebar": true,
    "alwaysShowTabs": true,
    "showTabsFullscreen": true,
    "useAcrylicInTabRow": false,
    "showTerminalTitleInTitlebar": false,
    "alwaysOnTop": false,
    "tabWidthMode": "equal",
    "disableAnimations": false,
    "alwaysShowNotificationIcon": false,
    "minimizeToNotificationArea": false,
    "autoHideWindow": false,
    "showAdminShield": true,
    "compatibility.enableUnfocusedAcrylic": true,

    // Themes (not in UI)
    "themes": [],

    // Color schemes
    "schemes": [],

    // Rendering
    "rendering.graphicsAPI": "automatic",
    "rendering.disablePartialInvalidation": false,
    "rendering.software": false,

    // Compatibility
    "compatibility.allowHeadless": false,
    "compatibility.textMeasurement": "graphemes",

    // Actions
    "actions": [
        {
            "id": "User.selectCommandNext",
            "command": { "action": "selectCommand", "direction": "next" }
        },
        {
            "id": "User.selectCommandPrevious",
            "command": { "action": "selectCommand", "direction": "prev" }
        },
        {
            "id": "User.selectOutputNext",
            "command": { "action": "selectOutput", "direction": "next" }
        },
        {
            "id": "User.selectOutputPrevious",
            "command": { "action": "selectOutput", "direction": "prev" }
        },
        {
            "id": "User.scrollToMarkNext",
            "command": { "action": "scrollToMark", "direction": "next" }
        },
        {
            "id": "User.scrollToMarkPrevious",
            "command": { "action": "scrollToMark", "direction": "previous" }
        },
        {
            "id": "User.splitPane",
            "command": { "action": "splitPane", "split": "auto" }
        }
    ],

    // Key bindings
    "keybindings": [
        { "id": "User.scrollToMarkNext", "keys": "ctrl+down" },
        { "id": "User.scrollToMarkPrevious", "keys": "ctrl+up" },
        { "id": "User.splitPane", "keys": "alt+shift+d" }
    ],

    // New Tab Menu
    "newTabMenu": [
        {
            "type": "remainingProfiles"
        },
        {
            "type": "folder",
            "name": "Visual Studio",
            "icon": "ms-appx:///ProfileIcons/{550ce7b8-d500-50ad-8a1a-c400c3262db3}.png",

            "allowEmpty": false,
            "inline": "never",

            "entries": [
                {
                    "type": "matchProfiles",
                    "name": null,
                    "source": "Windows.Terminal.VisualStudio",
                    "commandline": null
                }
            ]
        }
    ],

    // Advanced (not in UI)
    "disabledProfileSources": ["Windows.Terminal.Azure"],
    "startupActions": "", // None
    "experimental.useBackgroundImageForWindow": false,

    // Profiles
    "profiles": {
        "defaults": {
            // General
            "startingDirectory": null, // Use parent process directory
            "icon": null, // Use profile default
            "tabTitle": null, // Use profile name as tab title
            "elevate": false,

            // Appearance: Text
            "colorScheme": {
                "dark": "Campbell",
                "light": "Campbell"
            },
            "font": {
                "face": "Hack Nerd Font",
                "size": 10,
                "cellHeight": "1.2",
                "cellWidth": "0.6",
                "weight": "normal",
                "builtinGlyphs": true,
                "colorGlyphs": true
            },
            "experimental.retroTerminalEffect": false,
            "adjustIndistinguishableColors": "never",

            // Appearance: Cursor
            "cursorShape": "bar",
            "cursorColor": null, // Use scheme color

            // Appearance: Background image
            "backgroundImage": null, // No background image
            "backgroundImageStretchMode": "uniformToFill",
            "backgroundImageAlignment": "center",
            "backgroundImageOpacity": 1,

            // Appearance: Text Formatting
            "intenseTextStyle": "bright",

            // Appearance: Transparency
            "opacity": 90,
            "useAcrylic": false,

            // Appearance: Window
            "padding": "8",
            "scrollbarState": "visible",

            // Terminal Emulation
            "suppressApplicationTitle": false,
            "compatibility.input.forceVT": false,
            "compatibility.allowDECRQCRA": false,
            "compatibility.allowOSC52": true,
            "answerbackMessage": null, // Empty

            // Advanced
            "antialiasingMode": "grayscale",
            "altGrAliasing": true,
            "snapOnInput": true,
            "historySize": 10000,
            "closeOnExit": "graceful",
            "bellStyle": "none",
            "bellSound": [], // Default system sound
            "experimental.rightClickContextMenu": false,
            "showMarksOnScrollbar": false,
            "autoMarkPrompts": true,
            "compatibility.reloadEnvironmentVariables": true,
            "experimental.repositionCursorWithMouse": false,
            "experimental.rainbowSuggestions": false,
            "pathTranslationStyle": "none"
        },

        "list": [
            {
                "name": "PowerShell",
                "source": "Windows.Terminal.PowershellCore",
                "guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}", // Default GUID

                "commandline": "\"%ProgramFiles%\\PowerShell\\7\\pwsh.exe\" -NoLogo",
                "showMarksOnScrollbar": true
            },
            {
                "name": "Windows PowerShell",
                "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}", // Default GUID

                "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -NoLogo",
                "showMarksOnScrollbar": true
            },
            {
                "name": "Command Prompt",
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}", // Default GUID

                "commandline": "%SystemRoot%\\System32\\cmd.exe /K \"%DOTFILES%\\cmd\\SetupEnv.cmd\"",
                "closeOnExit": "always",
                "showMarksOnScrollbar": true
            },
            {
                "name": "Ubuntu",
                "source": "Microsoft.WSL", // Fragment
                "guid": "{00000000-0000-0000-0000-000000000000}",
                "icon": "C:\\Users\\SDL\\AppData\\Local\\wsl\\{00000000-0000-0000-0000-000000000000}\\shortcut.ico",

                "colorScheme": "Solarized Dark",
                "closeOnExit": "always",
                "pathTranslationStyle": "wsl"
            },
            {
                "name": "Kali",
                "source": "Microsoft.WSL", // Fragment
                "guid": "{00000000-0000-0000-0000-000000000000}",
                "icon": "C:\\Users\\SDL\\AppData\\Local\\wsl\\{00000000-0000-0000-0000-000000000000}\\shortcut.ico",

                "colorScheme": "Solarized Dark",
                "closeOnExit": "always",
                "pathTranslationStyle": "wsl"
            },
            {
                "name": "Git Bash",
                "guid": "{2ece5bfe-50ed-5f3a-ab87-5cd4baafed2b}",
                "icon": "%ProgramFiles%\\Git\\mingw64\\share\\git\\git-for-windows.ico",

                "commandline": "\"%ProgramFiles%\\Git\\usr\\bin\\bash.exe\" --login -i",
                "startingDirectory": "%USERPROFILE%",
                "closeOnExit": "always",
                "pathTranslationStyle": "msys2"
            },
            {
                "name": "VS 2022 - Cmd",
                "source": "Windows.Terminal.VisualStudio",
                // Generated from VS instance ID which is unique to an install
                "guid": "{00000000-0000-0000-0000-000000000000}",

                "startingDirectory": "%USERPROFILE%",
                "closeOnExit": "always"
            },
            {
                "name": "VS 2022 - PowerShell",
                "source": "Windows.Terminal.VisualStudio",
                // Generated from VS instance ID which is unique to an install
                "guid": "{00000000-0000-0000-0000-000000000000}",

                "startingDirectory": "%USERPROFILE%"
            },
            {
                "name": "Visual Studio Debug Console",
                "source": "VSDebugConsole", // Fragment
                "guid": "{16208362-94fc-5b1f-a491-5b2624d5ab56}",
                "hidden": true
            }
        ]
    }
}
